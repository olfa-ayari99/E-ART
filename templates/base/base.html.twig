<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>{% block title %}E-art{% endblock %}</title>
    <link
      rel="icon"
      href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>"
    />
    {# Run `composer require symfony/webpack-encore-bundle` to start using
    Symfony UX #} {% block stylesheets %}
    <link rel="stylesheet" href="/assetsFront/css/bootstrap.min.css" />
    <!-- Animate Min CSS -->
    <link rel="stylesheet" href="/assetsFront/css/animate.min.css" />
    <!-- Line Awesome CSS -->
    <link rel="stylesheet" href="/assetsFront/css/line-awesome.min.css" />  
    <!-- Owl Carousel CSS -->
    <link rel="stylesheet" href="/assetsFront/css/owl.carousel.min.css" />
    <!-- Odometer CSS -->
    <link rel="stylesheet" href="/assetsFront/css/odometer.css" />
    <!-- Slick CSS -->
    <link rel="stylesheet" href="/assetsFront/css/slick.min.css" />
    <!-- Popup CSS -->
    <link rel="stylesheet" href="/assetsFront/css/magnific-popup.min.css" />
    <!-- Style CSS -->
    <link rel="stylesheet" href="/assetsFront/css/style.css" />
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="/assetsFront/css/responsive.css" />

    {% endblock %}
  </head>
  <body data-spy="scroll" data-offset="120">
     <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container">
                <div class="logo">
                    <a href="app-landing.html">
                        <h3>E-ART</h3>
                    </a>
                </div>

                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
            
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a href="#home" class="nav-link">
                                Home
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#about" class="nav-link">
                                About
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#pricing " class="nav-link">
                                Pricing
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#team" class="nav-link">
                                Team
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#testimonial" class="nav-link">
                                Testimonial
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#blog" class="nav-link">
                                Blog
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#contact" class="nav-link">
                                Contact
                            </a>
                        </li>
                    </ul>

                    <div class="others-option">
                        <div class="d-flex align-items-center">
                            <div class="option-item">
                                <ul class="social-icon">
                                    <li>
                                        <a href="#" target="_blank">
                                            <i class="lab la-twitter"></i>
                                        </a>
                                    </li>
                
                                    <li>
                                        <a href="#" target="_blank">
                                            <i class="lab la-instagram"></i>
                                        </a>
                                    </li>
                
                                    <li>
                                        <a href="#" target="_blank">
                                            <i class="lab la-facebook-f"></i>
                                        </a>
                                    </li>
                                    
                                    <li>
                                        <a href="#" target="_blank">
                                            <i class="lab la-linkedin-in"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    {% block body %} 
    
    {% endblock %} 
    <!-- Start Copy Right Area -->
    <div class="copy-right">
        <div class="container">
            <div class="copy-right-content">
                <p>
                    Copyright © 2022 E-ART. All Rights Reserved by  
                    <a href="https://esprit.tn/" target="_blank">
                        ESPRIT
                    </a>
                </p>
            </div>
        </div>
    </div>
    <!-- End Copy Right Area -->
    {% block javascripts %}
    <!-- jQuery Min JS -->
    <script src="/assetsFront/js/jquery-3.5.1.min.js"></script>
    <!-- Popper Min JS -->
    <script src="/assetsFront/js/popper.min.js"></script>
    <!-- Bootstrap Min JS -->
    <script src="/assetsFront/js/bootstrap.min.js"></script>
    <!-- Owl Carousel JS -->
    <script src="/assetsFront/js/owl.carousel.min.js"></script>
    <!-- Odometer JS -->
    <script src="/assetsFront/js/odometer.min.js"></script>
    <!-- Appear JS -->
    <script src="/assetsFront/js/jquery.appear.js"></script>
    <!-- Slick JS -->
    <script src="/assetsFront/js/slick.min.js"></script>
    <!-- Popup JS -->
    <script src="/assetsFront/js/jquery.magnific-popup.min.js"></script>
    <!-- Form Ajaxchimp JS -->
    <script src="/assetsFront/js/jquery.ajaxchimp.min.js"></script>
    <!-- Form Validator JS -->
    <script src="/assetsFront/js/form-validator.min.js"></script>
    <!-- Contact JS -->
    <script src="/assetsFront/js/contact-form-script.js"></script>
    <!-- Main JS -->
    <script src="/assetsFront/js/main.js"></script>
    {% endblock %}
  </body>
</html>
